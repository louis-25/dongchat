#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Exception/Project/louis-25/dongchat/server/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_re_e6aa8ef5f9dd54c88b57712ed4c4c385/node_modules/typeorm/node_modules:/mnt/c/Exception/Project/louis-25/dongchat/server/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_re_e6aa8ef5f9dd54c88b57712ed4c4c385/node_modules:/mnt/c/Exception/Project/louis-25/dongchat/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Exception/Project/louis-25/dongchat/server/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_re_e6aa8ef5f9dd54c88b57712ed4c4c385/node_modules/typeorm/node_modules:/mnt/c/Exception/Project/louis-25/dongchat/server/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_re_e6aa8ef5f9dd54c88b57712ed4c4c385/node_modules:/mnt/c/Exception/Project/louis-25/dongchat/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-esm.js" "$@"
fi
